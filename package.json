{
	"name": "sadespresso.github.io",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --open",
		"dev:host": "vite dev --open --host",
		"start": "run-p dev typesafe-i18n",
		"start:host": "run-p dev:host typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n",
		"gh-pages": "pnpm run build && pnpx gh-pages -d build -t true",
		"gh-pages-actions": "pnpm run build && pnpx gh-pages -d build -t true -u \"github-actions-bot <support+actions@github.com>\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.30.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.8.6",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^4.5.5"
	},
	"dependencies": {
		"svelte-simple-modal": "^1.6.2",
		"typesafe-i18n": "^5.26.2"
	},
	"type": "module"
}
